{
  "name": "@types/sequelize",
  "version": "3.30.4",
  "private": true,
  "description": "Typescript Typings for Sequelize",
  "typings": "index.d.ts",
  "dependencies": {
    "@types/bluebird": "^3.5.5",
    "@types/validator": "^6.2.0"
  },
  "devDependencies": {
    "tslint": "^5.4.3",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.4",
    "typings": "^2.1.1"
  },
  "scripts": {
    "postinstall": "typings install && typings prune",
    "build": "tsc -p . && typings bundle -o dist/index.d.ts",
    "test": "tsc -p test",
    "lint": "tslint -c tslint.json index.d.ts 'lib/**/*.ts' 'test/**/*.ts'",
    "typedoc": "typedoc --module es6 --target es6 --includeDeclarations --excludeExternals --readme docs_index.md --name \"TypeScript definitions for Sequelize\" --mode file --out typedoc index.d.ts lib/data-types.d.ts typings/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/types/npm-sequelize.git"
  },
  "author": "Louay Alakkad",
  "contributors": [
    "Felix Becker <felix.b@outlook.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/types/npm-sequelize/issues"
  },
  "homepage": "https://typed-sequelize.surge.sh"
}
