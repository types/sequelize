{
  "compilerOptions": {
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "lib": [
      "es2015",
      "es2015.collection",
      "dom"
    ]
  },
  "include": [
    "lib/**/*.d.ts",
    "index.d.ts"
  ]
}
